#include <iostream>
#include <string.h>
using namespace std;

int main()
{
    int n,m;
    cin>>n>>m;
    for(int i=0;i<n;i++)
    {
        int j=0;int k=i;
            while(j<m&&j!=i)
            {
                cout<<char('A'+k);
                k--;
                j++;
            }
            while(j<m)
            {
                cout<<char('A'+j-i);
                j++;
            }
     cout<<endl;
    }

    return 0;
}
